    <ReturnValue type="double" description="Returns the quotient"/>
    <Category>Math</Category>
    <NumberOfExpectedParameters>2</NumberOfExpectedParameters>
    <ImplementingClassName>slogo.model.commands.math.QuotientCommand</ImplementingClassName>
    <Aliases>
        <Alias>/</Alias>
        <Alias/>
    </Aliases>
